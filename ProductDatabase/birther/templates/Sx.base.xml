<DeviceManifest>
	<SystemInfo>
		<SerialNumber value="{serialNumber}" />
		<DateOfManufacture value="{birth.device.timestamp}" />
		<DeviceTypeUID value="0x{birth.typeUID:08X}" />
		<ProductName value="{productName}" />
		<HwRev value="{hwRev}" />
		<MinFwRev value="{minFwRev}" />
		<PartNumber value="{partNumber}" />
    <BatchIDStr value="{device.batchId}" />
	</SystemInfo>

	<AAFilterInfo>
		<FilterResponse value="0" /> <!-- Butterworth -->
		<FilterOrder value="5" />
		<ClockToCornerRatio value="100" />
		<FMin value="1" />
		<FMax value="15000" />
		<Bypassable value="0" />
	</AAFilterInfo>

</DeviceManifest>
